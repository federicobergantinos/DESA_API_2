{
    "family": "Wallet",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::877306037129:role/LabRole",
    "taskRoleArn": "arn:aws:iam::877306037129:role/LabRole",
    "containerDefinitions": [
        {
            "name": "wallet-app",
            "image": "877306037129.dkr.ecr.us-east-1.amazonaws.com/wallet-serving:latest",
            "essential": true,
            "memory": 256,
            "cpu": 128,
            "portMappings": [
                {
                    "containerPort": 8080
                }
            ],
            "environment": [
                {
                    "name": "CODE",
                    "value": "superSecret"
                },
                {
                    "name": "POSTGRES_DB",
                    "value": "daidb"
                },
                {
                    "name": "POSTGRES_USER",
                    "value": "postgres"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "base1234"
                },
                {
                    "name": "NODE_ENV",
                    "value": "production"
                },
                {
                    "name": "POSTGRES_HOST",
                    "value": "wallet-serving-db-dbinstance-jg79hftmmnfg.ci6gprfovemt.us-east-1.rds.amazonaws.com"
                },
                {
                    "name": "POSTGRES_PORT",
                    "value": "5432"
                },
                {
                    "name": "AWS_S3_BUCKET_NAME",
                    "value": "wallet-desa-api-2"
                },
                {
                    "name": "AWS_DEFAULT_REGION",
                    "value": "us-east-1"
                },
                {
                    "name": "AWS_REGION",
                    "value": "us-east-1"
                },
                {
                    "name": "CORE_BANCARIO_QUEUE_URL",
                    "value": "https://sqs.us-east-1.amazonaws.com/877306037129/core_bancario_queue"
                },
                {
                    "name": "CRYPTO_QUEUE_URL",
                    "value": "https://sqs.us-east-1.amazonaws.com/877306037129/crypto_queue"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "Logs",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "Wallet"
                }
            }
        }
    ]
}